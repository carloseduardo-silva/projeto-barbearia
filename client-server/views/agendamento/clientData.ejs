 <h2> Insira Abaixo as Informações Sobre Você </h2>
  <section id="client-info">
  
    <form class="form-horizontal">
        
        <div class="form-group">
          <label for="nome" class="col-sm-2 control-label">Nome</label>
          <div class="col-sm-10">
            <input required type="name" class="form-control" id="nome" placeholder="Nome">
          </div>
        </div>

        <div class="form-group">
          <label for="email" class="col-sm-2 control-label">Email</label>
          <div class="col-sm-10">
            <input required type="email" class="form-control" id="email" placeholder="Email">
          </div>
        </div>
        

        <div class="form-group">
            <label for="telefone" class="col-sm-2 control-label">Telefone</label>
            <div class="col-sm-10">
              <input required type="telefone" class="form-control" id="telefone" placeholder="telefone">
            </div>

          </div>
       
        </div>
      </form>
  
  </section>
  <div id="botoes">
